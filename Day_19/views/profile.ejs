<html>

<head>
    <title>Profile page</title>
</head>

<body>
    <%- include('common/header') %>
    <h1>This is Profile page!!</h1>
    <h2>Welcome <%= user.name %>
    </h2>
    <h3>My email is <%= user.email %>
    </h3>
    <h4>
        <%= user.city %>
    </h4>
    <ul>
        <% user.skills.forEach((item) => { %>
            <li><%= item %></li>
        <% }) %>
    </ul>
</body>

</html>